{
  "name": "metricly-cli",
  "version": "0.7.0",
  "description": "Metricly CLI",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "lint": "tslint -p tslint.json",
    "test": "yarn run lint && nyc mocha",
    "prepublish": "yarn run compile && yarn run lint",
    "package": "yarn run compile && pkg .",
    "watch": "mocha-typescript-watch"
  },
  "bin": {
    "metricly": "js/bin/metricly.js"
  },
  "author": "TheConnMan",
  "license": "Apache-2.0",
  "dependencies": {
    "archiver": "^3.0.0",
    "caporal": "^1.2.0",
    "chalk": "^2.4.2",
    "clean-deep": "^3.0.2",
    "extend": "^3.0.2",
    "inquirer": "^6.3.1",
    "is-url": "^1.2.4",
    "json-dup-key-validator": "^1.0.2",
    "json-stable-stringify": "^1.0.1",
    "moment": "^2.24.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "tty-table": "^2.7.0",
    "update-notifier": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/extend": "^3.0.1",
    "@types/inquirer": "6.0.2",
    "@types/is-url": "^1.2.28",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.43",
    "@types/update-notifier": "^2.5.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.1.1",
    "pkg": "^4.4.0",
    "ts-node": "^8.1.0",
    "tslint": "~5.16.0",
    "typescript": "^3.4.5"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  }
}
